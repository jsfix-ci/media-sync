{
  "schedule": "0 */15 * * * *",
  "provider": {
    "type": "dropbox",
    "source": "/Camera Uploads",
    "token_store": "/data/.dropbox-token"
  },
  "handlers": [
    {
      "extensions": ["jpg", "jpeg", "png"],
      "target": "/media/photo"
    },
    {
      "extensions": ["mp4"],
      "target": "/media/video"
    }
  ]
}
